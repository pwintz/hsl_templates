{
    "ltex.latex.commands": {
        "\\tableofcontents[]": "ignore", 
        "\\@ifclassloaded{}{}{}": "ignore",  
        "\\newcommand{}{}": "ignore",  
        "\\renewcommand{}{}": "ignore",  
        "\\nbd{}": "dummy", 
        "\\nbd": "dummy", 
        "\\lhs{}": "dummy", 
        "\\lhs": "dummy", 
        "\\rhs{}": "dummy", 
        "\\rhs": "dummy", 
        "\\th{}": "ignore", 
        "\\newbool{}": "ignore",  
        "\\setbool{}": "ignore",
        "\\setbool{}{}": "ignore",
        "\\crefformat{}{}": "ignore",  
        "\\Crefformat{}{}": "ignore", 
        "\\crefmultiformat{}{}{}{}{}": "ignore",  
        "\\Crefmultiformat{}{}{}{}{}": "ignore",  
        "\\newenvironment{}[][]{}{}": "ignore", 
        "\\newenvironment{}[]{}{}": "ignore", 
        "\\newenvironment{}": "ignore",  
        "\\NewDocumentCommand{}{}{}": "ignore", 
        "\\DeclarePairedDelimiterX{}[]{}{}": "ignore", 
        "\\DeclarePairedDelimiter{}{}{}": "ignore", 
        "\\setbeamertemplate{}[]{}": "ignore", 
        "\\setbeamercolor{}{}": "ignore",  
        "\\setbeamerfont{}{}": "ignore", 
        "\\usebeamercolor[]{}": "ignore", 
        "\\usebeamertemplate{}][]": "ignore",
        "\\setbeamertemplate{}{}": "ignore", 
        "\\setbeamertemplate{}[]": "ignore", 
        "\\usebeamertemplate{}[]{}": "ignore",
        "\\setbeamerfont{}": "ignore", 
        "\\addtolength{}{}": "ignore", 
        "\\lstinputlisting{}": "ignore",
        "\\problem{}": "ignore",
        "\\subproblem{}": "ignore",
        "\\code{}": "ignore",
        "\\cite{}": "dummy",
        "\\nocite{}": "ignore",
        "\\deleted{}": "ignore",
        "\\comment{}": "ignore",
        "\\chcomment{}": "ignore",
        "\\inlinetodo{}": "ignore",
        "\\todo{}": "ignore",
        "\\todo[]{}": "ignore",
        "\\newabbreviation{}{}{}": "ignore",
        "\\mathtoolsset{}": "ignore",
        "\\qedhere{}": "ignore", // This doesn't seem to work.
        "\\qedhere": "ignore", // This doesn't seem to work.
    },
    "ltex.latex.environments": {
        "lstlisting": "ignore",
        "verbatim": "ignore", 
        "comment": "ignore"
    },
    "ltex.diagnosticSeverity": {
        "default": "information", 
        "PASSIVE_VOICE": "hint",
        "ENGLISH_WORD_REPEAT_BEGINNING_RULE": "hint",
        "MORFOLOGIK_RULE_EN_US": "warning" // Spelling errors.
    },
    "ltex.enabled": [
        "bibtex",
        "context",
        "context.tex",
        "latex",
        // "markdown" // Disable checking of .md files.
    ],
    "ltex.additionalRules.enablePickyRules": true,
    "latex-workshop.latex.autoBuild.cleanAndRetry.enabled": false,
    "latex-workshop.hover.preview.cursor.enabled": false,
    "latex-workshop.hover.preview.enabled": false,
    "latex-workshop.latex.external.build.args": [
        "-interaction=nonstopmode"
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk ðŸ”ƒ",
            "tools": [
                "latexmk"
            ]
        },
    ],
    "latex-workshop.latex.tools": [

        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        }
    ],
    "files.exclude": {
        "**/*.4ct": true,
        "**/*.4tc": true,
        "**/*.aux": true,
        "**/*.fdb_latexmk": true,
        "**/*.idv": true,
        "**/*.lg": true,
        "**/*.log": true,
        "**/*.synctex.gz": true,
        "**/*.tmp": true,
        "**/*.xref": true,
        "**/*.bbl": true,
        "**/*.fls": true,
        "**/*.loc": true,
        "**/*.out": true,
        "**/*.run.xml": true,
        "**/*.soc": true,
        "**/*.bcf": true,
        "**/*.nav": true,
        "**/*.toc": true,
        "**/*.snm": true,
        "**/*.blg": true
    },
    "latex-workshop.latex.pdfWatch.delay": 150
}
